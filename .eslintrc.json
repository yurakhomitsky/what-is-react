{
    "env": {
        "browser": true,
        "es6": true,
        "react": true
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "object-curly-spacing": "off",
        "indent": "off",
        "linebreak-style": "off",
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "new-cap": "off",
        "max-len": "off"
    }
}